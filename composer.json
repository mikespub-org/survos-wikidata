{
  "name": "mikespub/survos-wikidata",
  "description": "A PHP client for working with Wikidata API.",
  "keywords": [
    "wikidata",
    "client",
    "php"
  ],
  "homepage": "https://github.com/mikespub-org/survos-wikidata",
  "license": "MIT",
  "authors": [
    {
      "name": "Arhey",
      "role": "developer"
    },
    {
      "name": "tacman",
      "role": "developer"
    },
    {
      "name": "mikespub",
      "role": "developer"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "require": {
    "php": ">=8.2",
    "illuminate/collections": "^12.0",
    "symfony/http-client": "^6.4||^7.3"
  },
  "autoload": {
    "psr-4": {
      "Wikidata\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
        "tests/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5",
    "phpstan/phpstan": "^2.1",
    "rector/rector": "^2.2"
  }
}
