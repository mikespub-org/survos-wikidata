{
  "name": "mikespub/survos-wikidata",
  "description": "A PHP client for working with Wikidata API.",
  "keywords": [
    "wikidata",
    "client",
    "php"
  ],
  "homepage": "https://github.com/mikespub-org/survos-wikidata",
  "license": "MIT",
  "authors": [
    {
      "name": "Arhey",
      "role": "developer"
    },
    {
      "name": "tacman",
      "role": "developer"
    },
    {
      "name": "mikespub",
      "role": "developer"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "require": {
    "php": "^8.2",
    "illuminate/collections": "^10.0|^11.0",
    "symfony/http-client": "^6.4||^7.0"
  },
  "autoload": {
    "psr-4": {
      "Wikidata\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
        "tests/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5||^11.4",
    "phpstan/phpstan": "^1.12",
    "rector/rector": "^1.2"
  }
}
